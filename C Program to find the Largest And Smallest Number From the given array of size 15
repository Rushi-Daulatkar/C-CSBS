#include <stdio.h>
void main() {
    int a[15];
    int i,max,min;
    printf("Enter any Fiftteen  values:");
    for(i=0;i<15;i++)
    {
        scanf("%d",&a[i]);
    }
    max=a[0];
    min=a[0];
    for(i=0;i<15;i++)
    {
        if(a[i]>max)
        {
            max=a[i];
        }
    }
    printf("\n Largest Number:%d",max);
    
    for(i=0;i<15;i++)
    {
        if(a[i]<min)
        {
            min=a[i];
        }
    }
    
    printf("\n Smallest Number:%d",min);
}
