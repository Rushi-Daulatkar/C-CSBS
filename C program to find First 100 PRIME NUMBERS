#include<stdio.h>

int main()
{
  int i, num, count, prime=0; 

  printf("First 100 Prime Numbers are: \n");
  for(num=2 ; prime<100 ; num++)
  
  {
    count = 0;
    
    for (i = 2; i <= num/2; i++)
    {
        
	if(num%i == 0)
  	  count++;
    }
    
    if(count == 0)
    {
	printf(" %d ",num);
	prime++;
    }  

  }
  return 0;
}
