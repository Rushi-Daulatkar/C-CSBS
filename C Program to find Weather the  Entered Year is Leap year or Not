#include <stdio.h>

int main()
{
   int yr;
   printf("\n Enter ant Year:");
   scanf("\n %d",&yr);
   
   if(yr%100==0)
   {
       printf("\n century year %d",yr);
   }
   
   if(yr%400==0)
   {
       printf("\n century year %d is a leap year",yr);
   }else
   {
       if(yr%4==0)
       {
           printf("\n year %d is a leap year",yr);
       }
       else
       {
           printf("\n%d is not a Century year",yr);
        printf("\n%d is not a leap year",yr);
           
       }
       
   }
    return 0;
}
