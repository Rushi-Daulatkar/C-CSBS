//This code cointains addition of two numbers
//The Two Numbers Are Taken From User 

//The code is as Follows 


#include <stdio.h>

int main() 
{
    int a,b;
    printf("Enter any Two Numbers :");
    scanf("%d",&a);
    
    printf("Enter any Two Numbers :");
    scanf("%d",&b);
    
    printf("%d + %d = %d",a,b,a+b);

}
