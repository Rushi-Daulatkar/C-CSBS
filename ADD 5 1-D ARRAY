#include <stdio.h>

int main()
{
  
    int a[10];
    int b[10];
    int c[10];
    int d[10];
    int e[10];
    int i,size;
    
     printf("Enter the size of array");
     scanf("%d",&size);
    
    
    printf("Enter elements for array a");
    for(i=0;i<size;i++)
    {
        scanf("%d",&a[i]);
    }
    
     printf("Enter elements for array b");
    for(i=0;i<size;i++)
    {
        scanf("%d",&b[i]);
    }
    
     printf("Enter elements for array c");
    for(i=0;i<size;i++)
    {
        scanf("%d",&c[i]);
    }
    
     printf("Enter elements for array d");
    for(i=0;i<size;i++)
    {
        scanf("%d",&d[i]);
    }
    
    printf("The Addition is :\n");
    for(i=0;i<size;i++)
    {
        e[i]=a[i]+b[i]+c[i]+d[i];
    }
    
    for(i=0;i<size;i++)
    {
        printf("\t%d",e[i]);
    }
    return 0;
}
