#include <stdio.h>

int main() 
{
    char ch;
   
    printf("Enter any Character\n");
    scanf("%c",&ch);
    
    switch(ch)
    {
        case 'a':
        case 'e':
        case 'i':
        case 'o':
        case 'u':
        case 'A':
        case 'E':
        case 'I':
        case 'O':
        case 'U':printf("\nIt's a Vowel\n");
        break;
        default:printf("It's a Consonent\n");
    }
}
